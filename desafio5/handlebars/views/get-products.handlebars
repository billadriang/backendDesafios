{{#if products}}
    <table class="table">
        <thead>
          <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Miniatura</th>
          </tr>
        </thead>
        <tbody class="table-group-divider">
    {{#each products}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img src={{this.thumbnail}} width="60"></td>
        </tr>
    {{/each}}
            </tbody>
        </table>
{{else}}
<p>Nada aun compa</p>
{{/if}}
<a href="/" class="btn btn-dark">Crea un producto</a>